{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Warning"
    },
    "Tracor": {
      "LogLevel": {
        "Default": "Trace",
        "ReadAndWrite": "Debug"
      }
    },
    "TracorPublic": {
      "LogLevel": {
        "Default": "Information",
        "ReadAndWrite": "Trace"
      }
    },
    "TracorPrivate": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "Tracor": {
    "IsEnabled": true,
    "IsEmergencyLogging": false,
    "ApplicationName": "tracor-sample",
    "FileSink": {
      "Period": "00:05:00",
      "FileName": "log-{ApplicationName}_{TimeStamp}.jsonl",
      "Compression": "None",
      "CleanupEnabled": true
    }
  },
  "AllowedHosts": "*"
}
