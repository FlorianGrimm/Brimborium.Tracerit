{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "PublicTracor": {
      "Default": "Information"
    },
    "PrivateTracor": {
      "Default": "Information"
    }
  },
  "Tracor": {
    "IsEnabled": true,
    "IsEmergencyLogging": false,
    "ApplicationName": "tracor-sample",
    "SinkFile": {
      "Period": "00:01:00",
      "FileName": "log-{ApplicationName}_{TimeStamp}.jsonl",
      "Compression": "gzip",
      "CleanupEnabled": true
    }
  },
  "AllowedHosts": "*"
}
