{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Warning"
    },
    "Tracor": {
      "LogLevel": {
        "Default": "Trace",
        "ReadAndWrite": "Debug"
      }
    },
    "TracorPublic": {
      "LogLevel": {
        "Default": "Information",
        "ReadAndWrite": "Trace"
      }
    },
    "TracorPrivate": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "Tracor": {
    "IsEnabled": true,
    "IsEmergencyLogging": false,
    "ApplicationName": "benchmark"
  },
  "AllowedHosts": "*"
}
