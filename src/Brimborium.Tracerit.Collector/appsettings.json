{
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "http://*:4319"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Tracor": {
    "IsEnabled": true,
    "ApplicationName": "TracoritCollector",
    "FileSink": {
      "Period": "00:05:00",
      "Period2": "06:00:00",
      "BaseDirectory": "c:/data/LogFiles/",
      "Directory": "{ApplicationName}",
      "FileName": "log-{ApplicationName}_{TimeStamp}.jsonl",
      "Compression": "gzip",
      "CleanupEnabled": true
    },
    "HttpSink": {
      "TargetUrl": "http://localhost:4319/_api/tracerit/v1/collector.http"
    }
  },
  "ServiceName": "TracoritCollector",
  "LogDirectory": "c:/temp/LogFiles"
}
