{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "PublicTracor": {
      "Default": "Information"
    },
    "PrivateTracor": {
      "Default": "Information"
    }
  },
  "Tracor": {
    "IsEnabled": true,
    "ApplicationName": "Sample",
    "FileSink": {
      "Period": "00:05:00",
      "BaseDirectory": "c:/data/LogFiles/",
      "Directory": "{ApplicationName}",
      "FileName": "log-{ApplicationName}_{TimeStamp}.jsonl",
      "Compression": "None",
      "CleanupEnabled": true
    },
    "HttpSink": {
      "TargetUrl": "http://localhost:4319/_api/tracerit/v1/collector.http"
    }
  },
  "AllowedHosts": "*"
}
